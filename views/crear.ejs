<%- include("template/cabecera", {tituloWeb: 'Nueva Botella'}) %>
<div class="container">
  <h2>Añadir Botella</h2>
  <form action="/" class="my-2" method="POST">
    <input type="number" placeholder="Id" class="form-control my-2" name="id">
    <input type="text" placeholder="Nombre" class="form-control my-2" name="nombre" />
    <input type="text" placeholder="Tipo Alcohol" class="form-control my-2" name="tipo" />
    <input type="text" placeholder="Graduación" class="form-control my-2" name="graduacion" />
    <input type="text" placeholder="Calidad" class="form-control my-2" name="calidad" />
    <button type="submit" class="btn btn-dark btn-block">Agregar</button>
  </form>
</div>
<%- include("template/footer") %>